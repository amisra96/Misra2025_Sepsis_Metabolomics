print(df_MetabPaths['Super Pathway'].value_counts())



df_MetabPaths['Log2FC'] = np.log2(df_MetabPaths['FoldChange'])
df_MetabPaths['absLog2FC'] = np.abs(df_MetabPaths['Log2FC'])
df_MetabPaths.head()


# Initialize circos sectors
sectors = {"Lipid": 431, "Amino Acid": 207, "Nucleotide": 66, "Xenobiotics": 62, "Carbohydrate": 51,
          "Cofactors and Vitamins":51, "Peptide":33}
sectorNames = ['Lipids','Amino Acids','Nucleotides','Xenobiotics','Carbohydrates',
              'Cofactors +  \nVitamins','Peptide','Energy']
circos = Circos(sectors, space=5)
# circos = Circos(sectors, space=5,start=-330,end=0)

# define visualization
vmax = 2.5
colors_DownNeutUp = ["#3399FF","#C0C0C0" ,"#FF6666"]


idx_count = -1
for sector in circos.sectors:
    idx_count +=1
#     vmin, vmax = 0, 3
        
    # Plot sector axis & name text
#     sector.axis(fc="none", lw=2, ec="black", alpha=0.5)
    sector.text(f"\n{sectorNames[idx_count]}\n", size=15)
    
    ## make data
    # define x-values for bargraph
    x = np.linspace(sector.start + 0.5, sector.end - 0.5, int(sector.size))
    #     print(x)
    
    # get df_MetabPaths and subset for SuperPathway, then sort metabolites in  ascending order
    df_MetabPaths2 = df_MetabPaths[df_MetabPaths['Super Pathway']==sector.name].sort_values(by='Log2FC').reset_index(drop=True)
    y = df_MetabPaths2['absLog2FC'].values
    
    # if y is above vmax, set it to vmax
    y2 = y
    y2[y2>vmax]=vmax
    
    # get indices of metabolites that are down (Log2FC<-1), neutral, or up (Log2FC>1)
    idx_down = df_MetabPaths2.index[df_MetabPaths2['Log2FC']<-1].tolist()
    idx_up = df_MetabPaths2.index[df_MetabPaths2['Log2FC']>1].tolist()
    idx_neut = np.setdiff1d(np.arange(df_MetabPaths2.shape[0]) , np.concatenate((idx_down, idx_up)))
    
    print('Idx = ',[len(idx_down),len(idx_neut),len(idx_up)])
    
    ## Plot data
    # Add tracks for bar graphs
    bar_track = sector.add_track((30, 90), r_pad_ratio=0.1)
    bar_track.axis()
#     bar_track.xticks_by_interval(1, tick_length=1, show_label=False)
    bar_track.bar(x, y2,linewidth=0.2,edgecolor='w')
#     bar_track.xticks_by_interval(15)
    

    # Metabolite Number Track
    metabNum_track = sector.add_track((20, 30))
#     metabNum_track = sector.add_track((90, 100))
    metabNum_track.axis(fc=ColorCycler(idx_count))
#     metabNum_track.xticks_by_interval(15)
    metabNum_track.text(f"{sector.size}",size=10,color='white')
    
    
    # Rectangle track (rectangles for up/down/neutral)
    rect_track = sector.add_track((90,100))
#     rect_track = sector.add_track((20,30))
    rect_track.xticks_by_interval(15)
    x1,x2 = 0,0
    for j in range(3):
        if j==0 and len(idx_down)>0: #plot down
            rect_size = len(idx_down)
            text_str =  str(len(idx_down))
            
        elif j ==1 and len(idx_neut)>0: # plot neuts
            rect_size = len(idx_neut)
            text_str =  str(len(idx_neut))
        
        elif j ==2 and len(idx_up)>0:#plot down: # plot ups
            rect_size = len(idx_up)
            text_str =  str(len(idx_up))
        else:
            continue
        
        # define start,end of rectangle
        x1, x2 = x1, x1 + rect_size
        # plot rectangle
        rect_track.rect(x1, x2, ec="black", lw=0.5, color=colors_DownNeutUp[j])
        rect_track.text(text_str, (x1 + x2) / 2, size=8, color="white")
        # update starting point of rectangle for next loop
        x1=x2

    
