import matplotlib
matplotlib.rcParams['pdf.fonttype'] = 42

df_PathwayDifferential = pd.read_csv('../processed/df_PathwayDifferential.csv')
df_MetabPaths = pd.read_csv('../processed/df_Metabolites+Pathways.csv')
df_MSEA = pd.read_csv('../processed/df_MSEA.csv')



# Merge teh dataframes using the Pathway column
df_MSEA_merge = df_PathwayDifferential.merge(df_MSEA,left_on='Pathway',right_on='Pathway')
df_MSEA_merge.head()


# compute % of metabolites that have  fold change, -log10(q-value)
df_MSEA_merge['PercentMetabsFoldChange'] = df_MSEA_merge.numFoldChange/df_MSEA_merge.numMetabolites*100
df_MSEA_merge['NegLog_q'] = -np.log(df_MSEA_merge['q-value'])
# drop rows in xenobiotics pathway
df_MSEA_merge=df_MSEA_merge[df_MSEA_merge.SuperPathway != 'Xenobiotics']
df_MSEA_merge=df_MSEA_merge.sort_values(by='NES',ascending=False).reset_index(drop=True)
df_MSEA_merge.head(50)


# make dot plot with q-value on x-axis, each pathway on y-axis, then size of marker as PercentMetabsFoldChange
# num_pathways =  8
