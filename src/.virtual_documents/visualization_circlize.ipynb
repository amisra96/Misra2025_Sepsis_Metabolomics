fig = plt.figure(figsize=(12,10))
plt.rcParams['font.size'] = 18. 

ax=plt.scatter(df_MSEA_merge.loc[idx_pathways,'NES'], df_MSEA_merge.loc[idx_pathways,'Pathway'],
         marker='o',s=df_MSEA_merge.loc[idx_pathways,'PercentMetabsFoldChange']*8,
            c=df_MSEA_merge.loc[idx_pathways,'NegLog_q'],cmap='Blues',vmin=3,vmax=7)

handles, labels = ax.legend_elements(prop="sizes",num=7)
labels = ['10%','20%','30%','40%','50%','60%']
legend = plt.legend(handles, labels, loc="lower right", title="Sizes") 

plt.colorbar()
plt.xlabel('Normalized Enrichment Score',fontsize=24)
