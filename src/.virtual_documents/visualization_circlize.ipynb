metab_def.head()





#Feed in the metabolite name (make sure it is in the same order as the Temperature in merged_data)
def metab_vals(merged_data, metab_def, metabolite, temp_thresh = 36):
    ##  Inputs
    # merged_data is a dataframe that includes all of the metabolite concentrations and
    # temperatures for each mouse, along with other clinical features
    # metab_def is a dataframe that contains the Chemical IDs, pathways, and
    # biochemical names of each of the 917 metabolite 
    # metabolite (string) is the individual metabolite of interest
    # temp_thresh (float) is the temperature cut off for the "high temperature" and "low temperature" groups
    
    ## Outputs
    #The desired output is a dataframe with a single row containing the metabolite's name, pathway it belongs to,
    # fold-change of NS to S, SNR score
    
    #Find the metabolite's Chemical ID
    metab_chemID = "X" + str(metab_def.loc[metab_def['BIOCHEMICAL'] == metabolite, "CHEMICAL ID"].values[0])
    #print(metab_chemID)
    # Create dataframe with chosen metabolite and temperature
    metabolite_df = pd.DataFrame(data={'Concentration': merged_data[metab_chemID].values,
                                       'Temperature': merged_data['Final_Temp'],
    })
    temp_high = metabolite_df[metabolite_df['Temperature'] > temp_thresh]
    temp_low = metabolite_df[metabolite_df['Temperature'] <= temp_thresh]

    # calculate t-test statistics, SNR, fold-change of non-survivor vs survivor
    #     print(temp_high)
    corr = stats.pearsonr(metabolite_df['Temperature'], metabolite_df['Concentration'])
   #print(corr)
    [t_stats, p_values] = stats.ttest_ind(temp_low.Concentration,temp_high.Concentration)
    #print(p_values)
    #print(t_stats)
    SNR = (temp_low.Concentration.mean()-temp_high.Concentration.mean()) / (temp_high.Concentration.std() + temp_low.Concentration.std())
    foldChange = temp_low.Concentration.mean()/temp_high.Concentration.mean()
    #print(SNR)
    
    metab_val_df = pd.DataFrame(data={
        "Metabolite": metabolite,
        'Super Pathway':  metab_def.loc[metab_def['BIOCHEMICAL'] == metabolite, "SUPER PATHWAY"].values[0],
        'Pathway': metab_def.loc[metab_def['BIOCHEMICAL'] == metabolite, "SUB PATHWAY"].values[0],
        "Absolute Value of R": abs(corr[0]),
        "R-value":corr[0],
        "Pearson R p-value": corr[1],
        "T-statistic": t_stats,
        "T-test p-value": p_values,
        "SNR": SNR,
        "FoldChange":foldChange
    }, index=[0])
    return(metab_val_df)

# example
print(metab_vals(merged_data, metab_def,'(14 or 15)-methylpalmitate (a17:0 or i17:0)', 36))



# iterate thru metabolite list, and assemble a dataframe
df_MetabPaths = pd.DataFrame([])

for metabolite in metabolites_list:
    df_MetabPaths = pd.concat([df_MetabPaths,metab_vals(merged_data,metab_def,metabolite,36)])
    
df_MetabPaths.reset_index(drop=True,inplace=True)






df_MetabPaths.sort_values(by='FoldChange').to_csv('../processed/df_Metabolites+Pathways.csv')
df_MetabPaths.head()


# iterate thru pathways and record number of metabolites that have high fold change (above 2 or below 0.5)
pathway_list = df_MetabPaths.Pathway.unique()
df_PathwayDifferential = []
for pathway in pathway_list:
    df_pathway = df_MetabPaths.loc[df_MetabPaths['Pathway']==pathway]
    superPathway = df_pathway['Super Pathway'].values[0]
    numFoldChange = df_pathway[df_pathway.FoldChange>2].shape[0] + df_pathway[df_pathway.FoldChange<0.5].shape[0]
    df_PathwayDifferential.append({'Pathway':pathway,'SuperPathway':superPathway,'numFoldChange':numFoldChange,
                                   'numMetabolites':df_pathway.shape[0],'MeanFoldChange':df_pathway.FoldChange.mean()})
df_PathwayDifferential = pd.DataFrame(df_PathwayDifferential).sort_values(by='numFoldChange',ascending=False)


df_PathwayDifferential.reset_index(drop=True,inplace=True)
df_PathwayDifferential.to_csv('../processed/df_PathwayDifferential.csv')
df_PathwayDifferential





